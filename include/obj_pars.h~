#ifndef __PARS_OBJ_FILE__
#define __PARS_OBJ_FILE__

typedef	struct		s_vortex
{
  int			i;
  int			total;
  float			ptn[99][3];
  struct s_vortex	*nxt;
  struct s_vortex	*prv;
} t_vr;

typedef	struct		s_face
{
  float			face[99][3][3];
  struct s_vortex	*nxt;
  struct s_vortex	*prv;
} t_face;

#endif
