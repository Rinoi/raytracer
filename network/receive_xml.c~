#include	"network.h"

int		receive_xml(t_sock *sock)
{
  char buffer[512];
  int ret;

  ret = 512;
  while (ret >= 512)
    {
      ret = read(sock->socket_fd, buffer, 512);
      write(STDOUT_FILENO, buffer, ret);
    }
  return (EXIT_SUCCESS);
}
